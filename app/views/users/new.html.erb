<h1>Welcome to the Private Events app</h1>
<p>Sign up if you don't have a user already</p>

<%= form_with model: @user, url: signup_path do |f| %>
  <%= f.label :name, "Username:" %>
  <%= f.text_field :name %>
  <%= f.label :email %>
  <%= f.text_field :email %>
  <%= f.label :name %>
  <%= f.submit "Sign up!" %>
<% end %>
